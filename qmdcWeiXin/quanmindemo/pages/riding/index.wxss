/* pages/riding/index.wxss */
view{
  font-weight: normal;
  font-family: "Helvetica Neue", Helvetica, Arial, "PingFang SC", "Hiragino Sans GB", "Heiti SC", "Microsoft YaHei", "WenQuanYi Micro Hei", sans-serif;
  font-size: 28rpx;
  line-height: 1.5;
}
.ridi-last{
  
  height: 100rpx;
  padding: 0 40rpx;
  font-size: 30rpx;
  line-height: 100rpx;
  color: #282828;
}
.ridi-ques{
  text-align: right;
  float: right;
  color: #ed5343;
}
.ridi-tit {
  text-align: center;
  padding: 50rpx 0;
}
.ridi-tit image{
  width: 54rpx;
  height: 36rpx;
  margin-right: 10rpx;
  vertical-align: top;
}
.ridi-tit text{
  color: #282828;
  font-size: 30rpx;
}
.ridi-time{
  text-align: center;
  color: #ed4e3d;
  font-size: 100rpx;
}
.ridi-box{
  margin: 20rpx 40rpx 60rpx;
  display: flex;
  position: relative;
  
}
.ridi-cont{
  width: 50%;
  text-align: center;
  color: #333;
  font-size: 50rpx;
}
.ridi-cont text{
  display: block;
}
.ridi-cont-bott{
  font-size: 28rpx;
  color: #626262;
}
.ridi-center{
  width: 2rpx;
  position: absolute;
  left: 50%;
  margin-left: -1rpx;
  background-color: #cbc4c4;
  height: 50rpx;
  top: 50%;
  margin-top: -25rpx;
}
.ridi-last2{
  background-color: #f7f7f7;
}


.pwd-box{
  background-color: #f7f7f7;
  padding: 30rpx 100rpx 50rpx;
}
.pwd-hide{
  display: none;
}
.pwd-tit{
  color: #999;
  font-size: 30rpx;
}
.pwd-tit text{
  color: #f9ce05;
  font-size: 36rpx;
}
.pwd-btn button{
  margin-top: 40rpx;
  color: #fff;
  width: 60%;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 32rpx;
  background-size: 100% 100%;
  outline: none;
  background-image: url("data:image/png;base64,");
  
  background-color: #f7f7f7;
}
button::after {
  border: 0;
} 
.pwd-list{
  margin-left: -5%;
  margin-top: 30rpx;
}
.pwd-list text{
  background-color: #fff;
  width: 20%;
  height: 134rpx;
  display: inline-block;
  text-align: center;
  margin-left: 5%;
  line-height: 134rpx;
  color: #000;
  font-size: 100rpx;
}